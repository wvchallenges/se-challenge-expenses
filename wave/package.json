{
  "name": "react-redux-postgres-starter",
  "version": "1.0.0",
  "description": "a simple react-redux starter with a restful postgres api",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack -p",
    "dev": "cross-env NODE_ENV=development nodemon ./server/server.js --ignore ./src/",
    "dev:debug": "cross-env NODE_ENV=development nodemon --inspect ./server/server.js --ignore ./src/",
    "dev:migrate": "knex migrate:latest --env development",
    "dev:seed": "knex seed:run --env development",
    "dev:db-setup": "npm run dev:migrate && npm run dev:seed",
    "start": "npm run build && cross-env NODE_ENV=production nodemon ./server/server.js"
  },
  "author": "vescogma",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-webpack-alias": "^2.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "basscss": "^8.0.3",
    "basscss-addons": "^1.0.0",
    "basscss-basic": "^1.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "es5-shim": "^4.5.9",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.1",
    "file-loader": "^0.9.0",
    "history": "^4.4.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.3",
    "postcss-loader": "^1.1.0",
    "postcss-modules-local-by-default": "^1.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-saga": "^0.12.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "csv-parse": "^1.1.7",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "knex": "^0.12.6",
    "multer": "^1.2.0",
    "nodemon": "^1.11.0",
    "pg": "^6.1.0"
  }
}
