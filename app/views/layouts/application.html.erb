<!DOCTYPE html>
<html>
  <head>
    <title>FileImporter</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
    <% if flash[:error] %>
        <div class="error">
          <%= flash[:error] %>
        </div>
    <% end %>

    <% if flash[:notice] %>
        <div class="notice">
          <%= flash[:notice] %>
        </div>
    <% end %>
    <%= yield %>
  </body>
</html>
